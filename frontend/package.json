    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "jsdom": "^27.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
