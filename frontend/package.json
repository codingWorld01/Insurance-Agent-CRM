{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rimraf .next out",
    "prebuild": "npm run clean",
    "postbuild": "echo \"Frontend build completed successfully\"",
    "export": "next export",
    "docker:build": "docker build -t insurance-crm-frontend .",
    "docker:run": "docker run -p 3000:3000 insurance-crm-frontend",
    "deploy:prepare": "npm run build",
    "health-check": "curl -f http://localhost:3000 || exit 1"
  },
  "dependencies": {
